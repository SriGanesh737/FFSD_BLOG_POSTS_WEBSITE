<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script src="https://kit.fontawesome.com/3e893105a3.js" crossorigin="anonymous"></script>
    <title>Contact Us</title>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.3/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="../css/style.css">
    <link rel="stylesheet" href="../css/contactus.css">
</head>
<body>


    <%- include('partials/loading') %>
    <%- include('partials/navbar') %>
    <%- include('partials/cursor') %>




    <div class="contactus-container">
        <div class="contactus-left">
            <h2 class="contactus-title">
                Contact Info
            </h2>
            <div class="contactus-links">
                <p class="single-link"><i class="fa-solid fa-location-crosshairs"></i>Indian Institute of Information Technology ,
                Sricity</p>
                <p class="single-link"><i class="fa-solid fa-phone"></i>+91 1234567890</p>
                <p class="single-link"><i class="fa-solid fa-envelope"></i> mindmeldhelp@gmail.com</p>
                <p class="single-link"><i class="fa-brands fa-facebook"></i>facebook</p>
                <p class="single-link"><i class="fa-brands fa-linkedin"></i>linked in</p>
                <p class="single-link"><i class="fa-brands fa-twitter"></i>twitter</p>
            </div>
            <div class="contactus-image">
              <img src="../images/Contact us-rafiki.svg" alt="">
            </div>
        </div>
        <div class="contactus-right">
            <h2 class="contactus-title">
                Send a message
            </h2>
          <form action="" class="contactus-form">
              <input placeholder="First Name" type="text" name="first-name" class="first-name">
              <input placeholder="Last Name" type="text" name="last-name" class="last-name">
              <input placeholder="Email" type="email" name="email" class="email">
              <input placeholder="Phone Number" type="text" name="phone-no" class="phone-no">
              <textarea name="message" id="contactus-msg" cols="30" rows="20" class="message"placeholder="Leave a Message"></textarea>

              <button class="contactus-submit" type="submit">Submit</button>
          </form>
        </div>
    </div>



    <%- include('partials/footer') %>

    <script src="../js/cursor.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="../js/navbar.js"></script>
    <script >
         window.onload = function () {
                particlesJS.load('particles-js', __dirname+'/particlesjs-config.json', function () {
                    console.log('callback - particles.js config loaded');
                });
            };
    </script>
</body>
</html>